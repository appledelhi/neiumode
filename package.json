{
    "name": "neiumode",
    "displayName": "NeiuMode",
    "description": "Modal editing mode for Colemak keyboard",
    "version": "0.1.0",
    "publisher": "jakesong",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.toggleNeiu",
                "title": "Toggle Neiu mode",
                "category": "Neiu mode"
            }
        ],
        "keybindings": [
            {
                "command": "extension.toggleNeiu",
                "key": "home",
                "when": "editorTextFocus"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}